<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Attendence_Event</display-name>
  <welcome-file-list>
  	<welcome-file>main1.jsp</welcome-file>
  	<welcome-file>main2.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Date</servlet-name>
    <servlet-class>logic.date.DateServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Member</servlet-name>
    <servlet-class>Account.loginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Date</servlet-name>
    <url-pattern> /first </url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Member</servlet-name>
    <url-pattern> /second </url-pattern>
  </servlet-mapping>
</web-app>